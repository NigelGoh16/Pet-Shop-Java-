public class Catt extends Product{
	private String Size;
	private String CProduct;
	private Service cat;
	private double Price;		

		public Catt(String cs, Service c, String pn, double pp, String cp, double P) {
			super(pn, pp);
			Size= cs;
			CProduct= cp;
			Price = P;
			cat = c;
		}
		
		public String getSize() {
			return Size;
		}
		
		public String getCProduct() {
			return CProduct;
		}
		
		public double getPrice() {
			return Price;
		}
		
		public Service getCat() {
			return cat;
		}
}
