
public class Catt extends Product{
	// TODO Auto-generated method stub
	
		private String Toy;
		private Service cat;
		private double Price;		
		
		public Catt() {
			Toy = "T";
			Price = 00.00;
			//cat = "c";
		}
		
		public Catt(String B, String F, String T, double P, Service c) {
			super(B, F);
			Toy = T;
			Price = P;
			cat = c;
		}
		
		public String getToy() {
			return Toy;
		}
		
		public void setToy(String T) {
			Toy = T;
		}
		
		
		public double getPrice() {
			return Price;
		}
		
		public void setPrice(double P) {
			Price = P;
		}
		
		public String getcat() {
			return cat.getGroom() + "/" + cat.getVet() + "/" + cat.getBreed();
		}
}
