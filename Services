public class Service {
	private String ServiceName;
	private double ServicePrice;
	
	public Service(String sn, double sp) {
		this.ServiceName= sn;
		this.ServicePrice= sp;
	}
	
	public String getServiceName() {
		return ServiceName;
	}
	
	public double getServicePrice() {
		return ServicePrice;
	}
	
}
