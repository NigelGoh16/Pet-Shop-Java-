
public class Dogg extends Product{
	// TODO Auto-generated method stub
	
	private String Size;
	private String Fetch;
	private Service dog;
	
	public Dogg() {
		Size = "S";
		Fetch = "f";
		//dog = "d";
	}
	
	public Dogg(String B, String F, String S, String f, Service d) {
		super(B, F);
		Size = S;
		Fetch = f;
		dog = d;
	}
	
	public String getSize() {
		return Size;
	}
	
	public void setSize(String S) {
		Size = S;
	}
	
	public String getFetch() {
		return Fetch;
	}
	
	public void setFetch(String f) {
		Fetch = f;
	}
	
	public String getdog() {
		return dog.getGroom() + "/" + dog.getVet() + "/" + dog.getBreed();
	}
}
