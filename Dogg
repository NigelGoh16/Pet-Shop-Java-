public class Dogg extends Product{	
	private String Size;
	private String DProduct;
	private Service service;
	private double Price;
	
	public Dogg(String S, Service ds, String pn, double pp, String dp, double P) {
		super(pn,pp);
		Size = S;
		DProduct = dp;
		Price = P;
		service= ds;
	}
	
	public String getSize() {
		return Size;
	}
	
	public String getDProduct() {
		return DProduct;
	}
	
	public double getPrice() {
		return Price;
	}
	
	public Service getService() {
		return service;
	}
}
