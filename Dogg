
public class Dogg extends Product{
	// TODO Auto-generated method stub
	
	private String Size;
	private String Fetch;
	private Service dog;
	private double Price;
	
	public Dogg() {
		Size = "S";
		Fetch = "f";
		Price = 00.00;
		//dog = "d";
	}
	
	public Dogg(String B, String F, String S, String f, double P, Service d) {
		super(B, F);
		Size = S;
		Fetch = f;
		Price = P;
		dog = d;
		
	}
	
	public String getSize() {
		return Size;
	}
	
	public void setSize(String S) {
		Size = S;
	}
	
	public String getFetch() {
		return Fetch;
	}
	
	public void setFetch(String f) {
		Fetch = f;
	}
	
	public double getPrice() {
		return Price;
	}
	
	public void setPrice(double P) {
		Price = P;
	}
	
	public String getdog() {
		return dog.getGroom() + "/" + dog.getVet() + "/" + dog.getBreed();
	}
}
